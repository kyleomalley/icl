{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "ec2:*",
                "s3:*",
                "iam:PassRole",
                "iam:ListRoles",
                "cloudformation:*",
                "cloudwatch:*",
                "autoscaling:*",
                "elasticloadbalancing:*",
                "route53:*"
            ],
            "Resource": "*"
        }
    ]
}